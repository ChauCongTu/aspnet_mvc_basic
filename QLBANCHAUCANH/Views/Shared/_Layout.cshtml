<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="description" content="">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <!-- The above 4 meta tags *must* come first in the head; any other head content must come *after* these tags -->
    <!-- Title  -->
    <title>@ViewBag.Title</title>
    <!-- Favicon  -->
    <link rel="icon" href="img/core-img/favicon.ico">
    <!-- Core Style CSS -->
    <link href="~/Content/Assets/css/core-style.css" rel="stylesheet" />
    <link href="~/Content/Assets/css/style.css" rel="stylesheet" />
</head>

<body>
    @RenderPage("~/Views/Shared/Blocks/Header.cshtml")
    @RenderBody()
    @RenderPage("~/Views/Shared/Blocks/Footer.cshtml")
</body>

<!-- ##### jQuery (Necessary for All JavaScript Plugins) ##### -->
<script src="~/Content/Assets/js/jquery/jquery-2.2.4.min.js"></script>
<!-- Ajax js -->
<script src="~/Content/Assets/js/ajax.js"></script>

<!-- Popper js -->
<script src="~/Content/Assets/js/popper.min.js"></script>
<!-- Bootstrap js -->
<script src="~/Content/Assets/js/bootstrap.min.js"></script>
<!-- Plugins js -->
<script src="~/Content/Assets/js/plugins.js"></script>
<!-- Active js -->
<script src="~/Content/Assets/js/active.js"></script>

</html>
